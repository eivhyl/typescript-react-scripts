{
	"compilerOptions": {
		"allowJs": true,
		"jsx": "preserve",
		"lib": ["dom", "es2017"],
		"maxNodeModuleJsDepth": 5,
		"module": "esnext",
		"allowSyntheticDefaultImports": true,
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"sourceMap": true,
		"strict": true,
		"target": "esnext"
	}
}
